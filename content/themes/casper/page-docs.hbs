              <div class="buttons">
                <div class="col-xs-6 gray-btn">
                  <p><span class="number">8</span> PRINT HOURS</p>
                </div>
                <div class="col-xs-6 white-btn">
                  <p><span class="number">80</span>g PLASTIC</p>
                </div>
              </div>
            </div>
            <div class="col-md-6">
              <img class="hovalin-top"src="/content/images/parts/hovalin_face.svg">
            </div>
          </div>
        </div>
      </div>

      <div id="the-bottom" class="std-padding part">
        <div class="container">
          <div class="row">
            <div class="col-md-6">
              <h2>The Bottom</h2>
              <h4>PARAMETERS</h4>
              <p>Layer Height..........................................0.1mm </p>
              <p>Infill: ...........................................................50%</p>
              <p>Perimeters:.....................................................3 </p>
              <div class="buttons">
                <div class="col-xs-6 gray-btn">
                  <p><span class="number">8</span> PRINT HOURS</p>
                </div>
                <div class="col-xs-6 white-btn">
                  <p><span class="number">80</span>g PLASTIC</p>
                </div>
              </div>
            </div>
            <div class="col-md-6">
              <img class="hovalin-bottom"src="/content/images/parts/hovalin_bottom.svg">
            </div>
          </div>
        </div>
      </div>

      <section class="post-content">
          {{content}}
      </section>

    </article>
</main>
{{/post}}
